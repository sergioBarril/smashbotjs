{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "prettier/prettier": "error",
    "linebreak-style": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-use-before-define": "error"
  }
}
