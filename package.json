    "winston": "^3.8.2"
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^28.1.3",
    "rewire": "^6.0.0"
