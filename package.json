{
  "name": "smashbotjs",
  "version": "2.0.0",
  "description": "Ranked bot for Smash Bros. Ultimate",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc ./src/**/*.ts --write .",
    "start": "doppler run -- npm run build && node build/index.js",
    "start:dev": "doppler run -- npx nodemon",
    "deploy:commands": "npm run build && doppler run -- node build/scripts/deploy-commands.js"
  },
  "author": "Tropped",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "axios": "^1.5.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
